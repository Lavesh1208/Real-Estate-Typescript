{
   "compilerOptions": {
      "target": "es2016",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "outDir": "dist",
      "sourceMap": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "typeRoots": ["./server/types", "./node_modules/@types"]
   }
}
